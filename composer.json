{
    "name": "dynamic/silverstripe-base-site",
    "type": "silverstripe-vendormodule",
    "description": "Base page types for SilverStripe websites",
    "keywords": [
        "silverstripe"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Dynamic",
            "email": "dev@dynamicagency.com",
            "homepage": "http://www.dynamicagency.com"
        }
    ],
    "require": {
        "axllent/silverstripe-cms-tweaks": "^2.0",
        "axllent/silverstripe-bootstrap-forms": "^2.0@dev",
        "axllent/silverstripe-email-obfuscator": "^2.0@dev",
        "axllent/silverstripe-scaled-uploads": "^2.1@dev",
        "axllent/silverstripe-version-truncator": "^2.0@dev",
        "dnadesign/silverstripe-elemental": "^4.0@dev",
        "dnadesign/silverstripe-elemental-list": "^1.1@dev",
        "dnadesign/silverstripe-elemental-userforms": "^2.0@dev",
        "dnadesign/silverstripe-elemental-virtual": "^1.0@dev",
        "dynamic/silverstripe-company-config": "^1.0@dev",
        "dynamic/core-tools": "^4.0@dev",
        "dynamic/silverstripe-elemental-testimonials": "^2.0@dev",
        "dynamic/silverstripe-elemental-blog": "^2.0@dev",
        "dynamic/silverstripe-elemental-customer-service": "^2.0@dev",
        "dynamic/silverstripe-elemental-embedded-code": "^2.0@dev",
        "dynamic/silverstripe-elemental-features": "^2.0@dev",
        "dynamic/silverstripe-elemental-flexslider": "^2.0@dev",
        "dynamic/silverstripe-elemental-image": "^2.0@dev",
        "dynamic/silverstripe-elemental-oembed": "^2.0@dev",
        "dynamic/silverstripe-elemental-promos": "^2.0@dev",
        "dynamic/silverstripe-elemental-section-navigation": "^2.0@dev",
        "dynamic/silverstripe-integration-config": "^1.0@dev",
        "dynamic/silverstripe-template-config": "^1.0@dev",
        "jonom/silverstripe-betternavigator": "^4.0@dev",
        "silverstripe/blog": "^3.0@dev",
        "silverstripe/elemental-fileblock": "^2.0@dev",
        "silverstripe/recaptcha": "^3.0@dev",
        "silverstripe/recipe-cms": "^4.0@dev",
        "silverstripe/redirectedurls": "dev-master",
        "silverstripe/securityreport": "^2.0@dev",
        "silverstripe/spamprotection": "^3.0@dev",
        "silverstripe/userforms": "^5.0@dev",
        "silverstripe/vendor-plugin": "^1.0@dev",
        "silverstripe/widgets": "^2.0@dev",
        "tractorcow/silverstripe-sitemap2": "^4.0@dev"
    },
    "require-dev": {
        "phpunit/PHPUnit": "^5.7",
        "squizlabs/php_codesniffer": "*",
        "phpmd/phpmd": "^2.6",
        "sebastian/phpcpd": "^3.0",
        "phploc/phploc": "^4.0",
        "pdepend/pdepend": "^2.5",
        "theseer/phpdox": "^0.11.0"
    },
    "config": {
        "process-timeout": 600
    },
    "extra": {
        "installer-name": "base-site",
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Dynamic\\Base\\": "src/",
            "Dynamic\\Base\\Test\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://repo.packagist.com/dynamic/"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
